<?xml version="1.0" encoding="UTF-8"?>

<odoo>

<record id="view_dms_data_database_tree" model="ir.ui.view">
	<field name="name">dcs_dms_data_database.tree</field>
	<field name="model">dcs_dms.data_database</field>
	<field name="arch" type="xml">
		<tree string="Files">
			<field name="id" />
			<field name="data" filename="id" widget="binary" />
		</tree>
	</field>
</record>

<record id="action_dms_data_database" model="ir.actions.act_window">
	<field name="name">Data Database</field>
	<field name="res_model">dcs_dms.data_database</field>
	<field name="view_mode">tree</field>
</record>

<!--<menuitem id="menu_dms_data_database" name="Database"-->
